<%= @user.username %>
<%= @user.email %>
<%= @user.picture %>
<%= @user.bio %>
<%= @user.contact_link %>

<% @user.reviews.each do |review| %>
  <div id="<%= review.id %>">
    <%= review.title %>
    <%= review.description %>
    <%= review.rating %>
    <%= link_to "Edit", edit_review_path(review), class:"btn btn-primary" %>
      <button class="btn btn-default userDelete">Delete</button>

